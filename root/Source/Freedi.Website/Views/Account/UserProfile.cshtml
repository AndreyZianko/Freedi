@model Freedi.Model.ViewModels.ClientProfileViewModel

@{
    ViewBag.Title = "UserProfile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <title>Profile</title>
    <link href="~/Content/AdminPanel/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/AdminPanel/sb-admin.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/Content/TemplateStyles/main_styles.css">
</head>
<div id="wrapper">
    <ul class="sidebar navbar-nav">
        <li class="nav-item">
            <a class="nav-link" ">
                <span>@Model.Name</span>
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link">
                <span>@Model.Address</span>
            </a>

        </li>

        <li class="nav-item">
           
                <a class="nav-link" data-ajax="true" data-ajax-mode="replace" data-ajax-update="#content" href="/Order/GetOrders">Orders</a>
        </li>

    </ul>
    <div id="content-wrapper">
        <div class="container-fluid">
            <div class="card border-white mb-3">
                <div class="card-body" id="content">

                </div>
            </div>

        </div>
    </div>

</div>

<a class="scroll-to-top rounded" href="#wrapper">
    <i class="fas fa-angle-up"></i>
</a>

